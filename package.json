{
  "name": "@camlegleiter/react-blink",
  "version": "0.0.1",
  "description": "Bringing back the classic <blink> tag",
  "main": "lib/index.js",
  "author": "Cameron Legleiter <camlegleiter@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "blink",
    "component"
  ],
  "scripts": {
    "build": "babel -s inline --ignore \"**/test\" --source-root src/ -d lib src/",
    "build:clean": "rimraf ./lib",
    "prebuild": "yarn build:clean",
    "prepublish": "yarn build",
    "setup": "yarn install",
    "postsetup": "yarn build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1"
  }
